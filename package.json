{
  "name": "cta-jobmanager",
  "version": "0.0.1",
  "description": "Job Manager Brick",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore lib/**/*.js test/**/*.js",
    "test": "mocha -R spec --recursive test",
    "codecoverage": "istanbul cover mocha -- -R spec --recursive test",
    "all": "npm run lint && npm run codecoverage",
    "gitlab-ci": "npm run all",
    "docs": "jsdoc2md \"lib/**/*.js\" > DOCUMENTATION.md && jsdoc --readme README.md lib -r -d docs"
  },
  "engines": {
    "node": "4.2.2"
  },
  "contributors": [
    "Pittawat Mongkonrat <pittawat.mongkonrat@thomsonreuters.com>",
    "Kiettisak Angkanawin <kiettisak.angkanawin@thomsonreuters.com>"
  ],
  "author": "",
  "license": "Apache-2.0",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.6",
    "mocha": "^2.5.3",
    "opener": "^1.4.1"
  },
  "sourceDependencies": {
    "cta-brick": "*",
    "cta-logger": "*",
    "cta-tools": "*"
  }
}
